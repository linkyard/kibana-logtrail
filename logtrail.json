{
  "es": {
    "default_index": "graylog*",
    "allow_url_parameter": false,
    "timezone": "UTC"
  },
  "tail_interval_in_seconds": 10,
  "max_buckets": 500,
  "nested_objects" : false,
  "default_time_range_in_days" : 0,
  "fields" : {
    "mapping" : {
        "timestamp" : "@timestamp",
        "display_timestamp" : "timestamp",
        "host" : "k8s-namespace",
        "program": "k8s-pod",
        "message": "message"
    }
  }
}
